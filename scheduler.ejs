<html>
    <head>
        <meta charset = "utf-8">
    </head>
    <body>
        <h2>APTW MMS Status List</h2>
        <ul class="list-group">
            <% for(var i = 0; i<scheduler.length; i++){%>
                <li class = "list-group-item">
                    <span>App Name: </span><%= scheduler[i].app_name %>
                    <br>
                    <span>Status: </span><%= scheduler[i].status %>
                    <br>
                    <span>Last run date: </span><%= scheduler[i].last_run_date %>
                </li>
            <% } %>
        </ul>
    </body>
</html>>